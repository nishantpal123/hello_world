<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Quota continueOnError="false" enabled="true" name="Q-quotalimit">
  <DisplayName>Q-quotalimit</DisplayName>
  <Properties/>
  <Allow count="3" countRef="request.header.allowed_quota"/>
  <Interval ref="request.header.quota_count">1</Interval>
  <Distributed>true</Distributed>
  <Synchronous>true</Synchronous>
  <TimeUnit ref="request.header.quota_timeout">minute</TimeUnit>
</Quota>
